                <%= image_tag 'uploadthumb.png', alt: t('images.image_preview'), class: 'uploadscreen_previewimgwrap' %>
              </div>
            </div>
          </td>
        </tr>       
      </table>
    </div>          
    <div id="screenupload_mainpage">
    </div>    
