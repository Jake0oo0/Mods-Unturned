= form_for([@submission, @download]) do |f|
  = render 'shared/errors', object: @download
  %p
    %b= f.label :name
    %br/
    = f.text_field :name
  %p
    %b= f.label :version
    %br/
    = f.text_field :version
  %p
    %b= f.label :download
    %br/
    = f.file_field :download, class: 'file'
  %p
    = f.submit 'Submit'
