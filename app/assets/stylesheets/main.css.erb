* {
	font-family: 'Arimo', Helvetica, sans-serif;
}
body {
	background-color: #2E2E2E;
	margin: 0;
	padding: 0;
}
a {
	text-decoration: none;
}

img[usemap], map area{
    outline: none;
}
.headerimagewrap {
	width: 860px;
	text-align: center;
	margin: 0 auto;
}
.headerwrap {
	width: 860px;
	text-align: center;
	margin: 0 auto;
}
.menu {
	display: inline-block;
	width: 420px;
	/* height: 70%;
	*/
	height: 29px;
	background-color: #F9F9F9;
	-webkit-border-radius: 5px;
	-moz-border-radius: 5px;
	border-radius: 5px;
}
.menuitem {
	height: 100%;
	width: 25%;
	background-color: #E2E2E2;
	float: left;
	border: none;
	margin: 0;
	background: rgba(249, 249, 249, 1);
	background: -moz-linear-gradient(top, rgba(249, 249, 249, 1) 0%, rgba(0, 0, 0, 1) 0%, rgba(249, 249, 249, 1) 0%, rgba(226, 226, 226, 1) 100%);
	background: -webkit-gradient(left top, left bottom, color-stop(0%, rgba(249, 249, 249, 1)), color-stop(0%, rgba(0, 0, 0, 1)), color-stop(0%, rgba(249, 249, 249, 1)), color-stop(100%, rgba(226, 226, 226, 1)));
	background: -webkit-linear-gradient(top, rgba(249, 249, 249, 1) 0%, rgba(0, 0, 0, 1) 0%, rgba(249, 249, 249, 1) 0%, rgba(226, 226, 226, 1) 100%);
	background: -o-linear-gradient(top, rgba(249, 249, 249, 1) 0%, rgba(0, 0, 0, 1) 0%, rgba(249, 249, 249, 1) 0%, rgba(226, 226, 226, 1) 100%);
	background: -ms-linear-gradient(top, rgba(249, 249, 249, 1) 0%, rgba(0, 0, 0, 1) 0%, rgba(249, 249, 249, 1) 0%, rgba(226, 226, 226, 1) 100%);
	background: linear-gradient(to bottom, rgba(249, 249, 249, 1) 0%, rgba(0, 0, 0, 1) 0%, rgba(249, 249, 249, 1) 0%, rgba(226, 226, 226, 1) 100%);
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#f9f9f9', endColorstr='#e2e2e2', GradientType=0 );
}

