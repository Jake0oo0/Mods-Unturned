.menu div:first-child {
  -webkit-border-top-left-radius: 5px;
  -webkit-border-bottom-left-radius: 5px;
  -moz-border-radius-topleft: 5px;
  -moz-border-radius-bottomleft: 5px;
  border-top-left-radius: 5px;
  border-bottom-left-radius: 5px;
}
.menu div:last-child {
  -webkit-border-top-right-radius: 5px;
  -webkit-border-bottom-right-radius: 5px;
  -moz-border-radius-topright: 5px;
  -moz-border-radius-bottomright: 5px;
  border-top-right-radius: 5px;
  border-bottom-right-radius: 5px;
}
.menuitem a {
  color: #1C1C1C;
}
.menuitem a:hover {
  text-decoration: underline;
}
.menutext {
  padding-top: 6px;
  vertical-align: middle;
  /* font-family: 'Arial', Helvetica, sans-serif;
  */
  font-family: 'Arimo', sans-serif;
  font-weight: bold;
  font-size: 15px;
  color: black;
}
#headerpage {
  height: 80px;
  background-color: #1C1C1C;
  position: relative;
  margin: 0 auto;
  text-align: center;
  background-image: url(<%= asset_path 'wallpaper.png' %>);
  background-position: top-left;
  background-repeat: repeat-x;
}

/* Home tab and Levels tab div */
#tabs {
  margin: 0 auto;
  width: 860px;
  height: 42px;
  margin-top: 10px;
  text-align: center;
}

/* Levels and Models heading text */
h2 {
  color: #FDFDFD;
  font-size: 40px;
  /* font-family: 'Trebuchet MS', Helvetica, sans-serif;
  */
  font-family: 'Arimo', sans-serif;
  font-weight: bold;
  text-align: center;
  margin: 0;
  padding-top: 20px;
  letter-spacing: 1px;
}

/* Specific div box for home tab */
.home {
  width: 120px;
  height: 40px;
  background-color: #5495D2;
  margin-left: 20px;
  float: left;
  -webkit-border-top-left-radius: 5px;
  -webkit-border-top-right-radius: 5px;
  -moz-border-radius-topleft: 5px;
  -moz-border-radius-topright: 5px;
  border-top-left-radius: 5px;
  border-top-right-radius: 5px;
  display: inline-block;
  font-family: 'Arimo', sans-serif;
  letter-spacing: 1px;
}

/* Specific div box for upload tab */
.upload {
  width: 120px;
  height: 40px;
  background-color: #5495D2;
  margin-right: 20px;
  float: right;
  -webkit-border-top-left-radius: 5px;
  -webkit-border-top-right-radius: 5px;
  -moz-border-radius-topleft: 5px;
  -moz-border-radius-topright: 5px;
  border-top-left-radius: 5px;
  border-top-right-radius: 5px;
  display: inline-block;
  font-family: 'Arimo', sans-serif;
  letter-spacing: 1px;
}
.home:hover {
  -o-transition: .1s ease-in-out;
  -ms-transition: .1s ease-in-out;
  -moz-transition: .1s ease-in-out;
  -webkit-transition: .1s ease-in-out; /* ...and now for the proper property */
  transition: .1s ease-in-out;
  background-color: #5A9FDF !important;
}
.upload:hover {
  -o-transition: .1s ease-in-out;
  -ms-transition: .1s ease-in-out;
  -moz-transition: .1s ease-in-out;
  -webkit-transition: .1s ease-in-out; /* ...and now for the proper property */
  transition: .1s ease-in-out;
  background-color: #5A9FDF !important;
}
.box h2 {
  text-align: center;
  position: relative;
  bottom: 2px;
  text-shadow: 1px 1px 2px rgba(50, 50, 50, 0.4);
}
.box {
  width: 880px;
  height: 85px;
  background: #88bfe8; /* Old browsers */
  background: rgba(62, 201, 217, 1);
  background: -moz-linear-gradient(top, rgba(62, 201, 217, 1) 0%, rgba(65, 176, 217, 1) 100%);
  background: -webkit-gradient(left top, left bottom, color-stop(0%, rgba(62, 201, 217, 1)), color-stop(100%, rgba(65, 176, 217, 1)));
  background: -webkit-linear-gradient(top, rgba(62, 201, 217, 1) 0%, rgba(65, 176, 217, 1) 100%);
  background: -o-linear-gradient(top, rgba(62, 201, 217, 1) 0%, rgba(65, 176, 217, 1) 100%);
  background: -ms-linear-gradient(top, rgba(62, 201, 217, 1) 0%, rgba(65, 176, 217, 1) 100%);
  background: linear-gradient(to bottom, rgba(62, 201, 217, 1) 0%, rgba(65, 176, 217, 1) 100%);
  filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#3ec9d9', endColorstr='#41b0d9', GradientType=0 );
  z-index: 5;
  margin-bottom: 10px;
  margin: 0 auto;
  -webkit-border-radius: 8px;
  -moz-border-radius: 8px;
  border-radius: 8px;
}

/* ==================================================
* Effect 1
* =============================================== */
.effect1 {
  -webkit-box-shadow: 0 10px 6px -6px #000;
  -moz-box-shadow: 0 10px 6px -6px #000;
  box-shadow: 0 10px 6px -6px #000;
}

/* ==================================================
* Effect 2
* =============================================== */
.effect3 {
  -webkit-box-shadow: -9px 0px 9px -6px #000;
  -moz-box-shadow: -9px 0px 9px -6px #000;
  box-shadow: -9px 0px 9px -6px #000;
}

/* Where community content goes to flourish */
.statement {
  float: left;
  margin: 0-auto;
  margin-left: 134px;
  text-align: center;
  color: white;
  font-family: 'Arial', Helvetica, sans-serif;
  padding-top: 8px
}

/* Home and Upload tabs */
h3 {
  text-align: center;
  margin: 0 auto;
  padding-top: 10px;
  color: white;
}
#mainpage {
  margin: 0 auto;
  background-color: rgb(219, 219, 219);
  /* #D6D6D6;
  */
  width: 860px;
  min-height: 240px;
  margin-bottom: 40px;
  -webkit-box-shadow: 0px 6px 11px 0px rgba(0, 0, 0, 0.75);
  -moz-box-shadow: 0px 6px 11px 0px rgba(0, 0, 0, 0.75);
  box-shadow: 0px 6px 11px 0px rgba(0, 0, 0, 0.75);
  border-top: 1px solid #C3C3C3;
  margin-bottom:15px;
}

/* #C1C1C1 original backgroud mainpage color */
/* div for left levels header box */
.contenthead1 {
  background: rgba(62, 201, 217, 1); /* Old browsers */
  background-color: rgba(62, 201, 217, 1);
  width: 100%;
  height: 85px;
  margin-bottom: 0px;
  -webkit-box-shadow: 0px 0px 6px 0px rgba(50, 50, 50, 0.12);
  -moz-box-shadow: 0px 0px 6px 0px rgba(50, 50, 50, 0.12);
  box-shadow: 0px 0px 6px 0px rgba(50, 50, 50, 0.12);
}

/* original color #54C1D2 next one was #28C7DC */
/* div for right header box */
.contenthead2 {
  background-color: #3CC3D4;
  width: 100%;
  height: 85px;
  margin-bottom: 20px;
  -webkit-border-top-right-radius: 5px;
  -webkit-border-bottom-right-radius: 5px;
  -moz-border-radius-topright: 5px;
  -moz-border-radius-bottomright: 5px;
  border-top-right-radius: 5px;
  border-bottom-right-radius: 5px;
  -webkit-box-shadow: 0px 0px 6px 0px rgba(50, 50, 50, 0.12);
  -moz-box-shadow: 0px 0px 6px 0px rgba(50, 50, 50, 0.12);
  box-shadow: 0px 0px 6px 0px rgba(50, 50, 50, 0.12);
}
.mainpageright {
  border-left: 1px solid rgb(205, 205, 205);
  float: right;
  /* margin-right: 20px;
  */
}
.mainpageleft {
  border-right: 1px solid rgb(200, 200, 200);
  float: left;
  /* margin-left: 20px;
  */
}

/* FOOTER SECTION */
#footerpage {
  height:140px; 
  margin-top:145px;
  background-color:#1C1C1C; 
  width:100%;
  border-top:1px solid rgb(20,20,20);
  /*background-image: url(footer.png);
  background-position:top-left;
  background-repeat: repeat-x;*/
}

.footertable {
  margin:0 auto;
  margin-top:15px;
}

.footertable a:link {
  color:#918B7F;
}

.footertable a:visited {
  color:#918B7F;
}

.footertable td {
  padding:5px 100px 2px 100px;
  color:#918B7F;
  font-weight:bold;
  border-bottom:1px solid #3D3930;
}

.footertable td:hover {
  background-color:rgb(20,20,20);
  -o-transition:.2s ease-in-out;
  -ms-transition:.2s ease-in-out;
  -moz-transition:.2s ease-in-out;
  -webkit-transition:.2s ease-in-out;
  /* ...and now for the proper property */
  transition:.2s ease-in-out;
}

.socialmedialogos {
  text-align:center;
  padding-top:20px;
}

td.socialmedialogos {
  border-bottom:none;
}

.socialmediaimg {
  display:inline-block;
  margin:20px;
}

/* CONTACT SECTION */
#contact-popup {
  position: relative;
  background: rgb(248,248,248);
  padding: 10px 20px 20px 20px;
  width: auto;
  max-width: 500px;
  margin: 20px auto;
  border-radius: 3px;
  font-family: 'Arimo', sans-serif;
  font-size:14px;
  font-weight:bold;
  color:rgb(50,50,50);
}

.contentboxes:last-child{
  height:300px;
}

.contactboxes {
  width:94%;
  margin-bottom:8px;
  background-color:rgb(253,253,253);
  outline: none;
  display: block;
  padding: 4px 8px;
  border: 1px dashed rgb(200,200,200);
  color: #3F3F3F;
  font-family: 'Droid Sans', Tahoma, Arial, Verdana sans-serif;
  font-size: 14px;
  -webkit-border-radius: 2px;
  -moz-border-radius: 2px;
  border-radius: 2px;
  -webkit-transition: background 0.2s linear, box-shadow 0.6s linear;
  -moz-transition: background 0.2s linear, box-shadow 0.6s linear;
  -o-transition: background 0.2s linear, box-shadow 0.6s linear;
  transition: background 0.2s linear, box-shadow 0.6s linear;
}

.contactboxes:focus {
  /*background: #F9F9F9;*/
  background-color:rgb(255,255,255);
  border: dashed 1px #74D5DF;
  -webkit-box-shadow: 2px 2px 7px #E8E8E8 inset;
  -moz-box-shadow: 2px 2px 7px #E8E8E8 inset;
  box-shadow: 2px 2px 7px #E8E8E8 inset;
}  

.contactsubmitbutton {
  height:25px;
  margin-top:5px;
}