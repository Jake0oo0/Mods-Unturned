/* Levels and Models heading text */
h2 {
  color: #FDFDFD;
  font-size: 40px;
  /* font-family: 'Trebuchet MS', Helvetica, sans-serif;
  */
  font-family: 'Arimo', sans-serif;
  font-weight: bold;
  text-align: center;
  margin: 0;
  padding-top: 20px;
  letter-spacing: 1px;
}

.box h2 {
  text-align: center;
  position: relative;
  bottom: 2px;
  text-shadow: 1px 1px 2px rgba(50, 50, 50, 0.4);
}
.box {
  width: 880px;
  height: 85px;
  background: #88bfe8; /* Old browsers */
  background: rgba(62, 201, 217, 1);
  background: -moz-linear-gradient(top, rgba(62, 201, 217, 1) 0%, rgba(65, 176, 217, 1) 100%);
  background: -webkit-gradient(left top, left bottom, color-stop(0%, rgba(62, 201, 217, 1)), color-stop(100%, rgba(65, 176, 217, 1)));
  background: -webkit-linear-gradient(top, rgba(62, 201, 217, 1) 0%, rgba(65, 176, 217, 1) 100%);
  background: -o-linear-gradient(top, rgba(62, 201, 217, 1) 0%, rgba(65, 176, 217, 1) 100%);
  background: -ms-linear-gradient(top, rgba(62, 201, 217, 1) 0%, rgba(65, 176, 217, 1) 100%);
  background: linear-gradient(to bottom, rgba(62, 201, 217, 1) 0%, rgba(65, 176, 217, 1) 100%);
  filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#3ec9d9', endColorstr='#41b0d9', GradientType=0 );
  z-index: 5;
  margin-bottom: 10px;
  margin: 0 auto;
  -webkit-border-radius: 8px;
  -moz-border-radius: 8px;
  border-radius: 8px;
}

/* ==================================================
* Effect 1
* =============================================== */
.effect1 {
  -webkit-box-shadow: 0 10px 6px -6px #000;
  -moz-box-shadow: 0 10px 6px -6px #000;
  box-shadow: 0 10px 6px -6px #000;
}

/* ==================================================
* Effect 2
* =============================================== */
.effect3 {
  -webkit-box-shadow: -9px 0px 9px -6px #000;
  -moz-box-shadow: -9px 0px 9px -6px #000;
  box-shadow: -9px 0px 9px -6px #000;
}

