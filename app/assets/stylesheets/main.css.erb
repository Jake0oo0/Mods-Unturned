* {
  font-family: 'Arimo', Helvetica, sans-serif;
}
body {
  background-color: #313438;
  margin: 0;
  padding: 0;
}
a {
  text-decoration: none;
}

.pagination {
  color: white;
}
.pagination a {
  color: white !important;
}
.headerimagewrap {
  width: 860px;
  text-align: center;
  margin: 0 auto;
}
.headerwrap {
  width: 860px;
  text-align: center;
  margin: 0 auto;
}
.item-tag {
  color: red;
  font-size: 12px;
  text-decoration: none;
}
h2 {
  color: #FDFDFD;
  font-size: 40px;
  font-weight: bold;
  text-align: center;
  margin: 0;
  padding-top: 20px;
  letter-spacing: 1px;
}
.box h2 {
  text-align: center;
  position: relative;
  bottom: 2px;
  text-shadow: 1px 1px 2px rgba(50, 50, 50, 0.4);
}
.box {
  width: 920px;
  height: 85px;
  background: #88bfe8;
  background: rgba(62, 201, 217, 1);
  background: -moz-linear-gradient(top, rgba(62, 201, 217, 1) 0%, rgba(65, 176, 217, 1) 100%);
  background: -webkit-gradient(left top, left bottom, color-stop(0%, rgba(62, 201, 217, 1)), color-stop(100%, rgba(65, 176, 217, 1)));
  background: -webkit-linear-gradient(top, rgba(62, 201, 217, 1) 0%, rgba(65, 176, 217, 1) 100%);
  background: -o-linear-gradient(top, rgba(62, 201, 217, 1) 0%, rgba(65, 176, 217, 1) 100%);
  background: -ms-linear-gradient(top, rgba(62, 201, 217, 1) 0%, rgba(65, 176, 217, 1) 100%);
  background: linear-gradient(to bottom, rgba(62, 201, 217, 1) 0%, rgba(65, 176, 217, 1) 100%);
  filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#3ec9d9', endColorstr='#41b0d9', GradientType=0);
  z-index: 5;
  margin-bottom: 10px;
  margin: 0 auto;
  -webkit-border-radius: 8px;
  -moz-border-radius: 8px;
  border-radius: 8px;
}
.effect1 {
  -webkit-box-shadow: 0 10px 6px -6px #000;
  -moz-box-shadow: 0 10px 6px -6px #000;
  box-shadow: 0 10px 6px -6px #000;
}
.effect3 {
  -webkit-box-shadow: -9px 0px 9px -6px #000;
  -moz-box-shadow: -9px 0px 9px -6px #000;
  box-shadow: -9px 0px 9px -6px #000;
}
.statement {
  float: left;
  margin: 0-auto;
  margin-left: 134px;
  text-align: center;
  color: white;
  font-family: 'Arial', Helvetica, sans-serif;
  padding-top: 8px
}
#mainpage {
	margin:0 auto; 
	background-color:rgb(219,219,219);
	width:900px; 
	min-height:240px; 
	margin-bottom:15px;
	-webkit-box-shadow: 0px 6px 11px 0px rgba(0, 0, 0, 0.45);
	-moz-box-shadow:    0px 6px 11px 0px rgba(0, 0, 0, 0.45);
	box-shadow:         0px 6px 11px 0px rgba(0, 0, 0, 0.45);
	border-top:1px solid #C3C3C3;
}
.contenthead1 {
  background: rgba(62, 201, 217, 1);
  background-color: rgba(62, 201, 217, 1);
  width: 100%;
  height: 85px;
  margin-bottom: 0px;
  -webkit-box-shadow: 0px 0px 6px 0px rgba(50, 50, 50, 0.12);
  -moz-box-shadow: 0px 0px 6px 0px rgba(50, 50, 50, 0.12);
  box-shadow: 0px 0px 6px 0px rgba(50, 50, 50, 0.12);
}

.logoheaderbar a.newprojectlink {
  color: #5d5d5d;
  margin: 0;
  display: inline-block;
  float: right;
  margin-top: 20px;
  font-size: 12px;
  margin-right: 20px;
  text-decoration: none;
}
.logoheaderbar a.newprojectlink:hover {
  color: #818181;
}



.contenthead2 {
  background-color: #3CC3D4;
  width: 100%;
  height: 85px;
  margin-bottom: 20px;
  -webkit-border-top-right-radius: 5px;
  -webkit-border-bottom-right-radius: 5px;
  -moz-border-radius-topright: 5px;
  -moz-border-radius-bottomright: 5px;
  border-top-right-radius: 5px;
  border-bottom-right-radius: 5px;
  -webkit-box-shadow: 0px 0px 6px 0px rgba(50, 50, 50, 0.12);
  -moz-box-shadow: 0px 0px 6px 0px rgba(50, 50, 50, 0.12);
  box-shadow: 0px 0px 6px 0px rgba(50, 50, 50, 0.12);
}
#footerpage {
  height: 140px;
  margin-top: 265px;
  background-color: #1C1C1C;
  width: 100%;
  border-top: 1px solid rgb(20, 20, 20);
}
.footertable {
  margin: 0 auto;
  margin-top: 15px;
}
.footertable a:link {
  color: #918B7F;
}
.footertable a:visited {
  color: #918B7F;
}
.footertable td {
  padding: 5px 100px 2px 100px;
  color: #918B7F;
  font-weight: bold;
  border-bottom: 1px solid #3D3930;
}
.footertable td:hover {
  background-color: rgb(20, 20, 20);
  -o-transition: .2s ease-in-out;
  -ms-transition: .2s ease-in-out;
  -moz-transition: .2s ease-in-out;
  -webkit-transition: .2s ease-in-out;
  transition: .2s ease-in-out;
}
.socialmedialogos {
  text-align: center;
  padding-top: 20px;
}
td.socialmedialogos {
  border-bottom: none;
}
.socialmediaimg {
  display: inline-block;
  margin: 20px;
}
#contact-popup {
  position: relative;
  background: rgb(248, 248, 248);
  padding: 10px 20px 20px 20px;
  width: auto;
  max-width: 500px;
  margin: 20px auto;
  border-radius: 3px;
  font-family: 'Arimo', sans-serif;
  font-size: 14px;
  font-weight: bold;
  color: rgb(50, 50, 50);
}
.contentboxes:last-child {
  height: 300px;
}
.contactboxes {
  width: 94%;
  margin-bottom: 8px;
  background-color: rgb(253, 253, 253);
  outline: none;
  display: block;
  padding: 4px 8px;
  border: 1px dashed rgb(200, 200, 200);
  color: #3F3F3F;
  font-family: 'Droid Sans', Tahoma, Arial, Verdana sans-serif;
  font-size: 14px;
  -webkit-border-radius: 2px;
  -moz-border-radius: 2px;
  border-radius: 2px;
  -webkit-transition: background 0.2s linear, box-shadow 0.6s linear;
  -moz-transition: background 0.2s linear, box-shadow 0.6s linear;
  -o-transition: background 0.2s linear, box-shadow 0.6s linear;
  transition: background 0.2s linear, box-shadow 0.6s linear;
}
.contactboxes:focus {
  background-color: rgb(255, 255, 255);
  border: dashed 1px #74D5DF;
  -webkit-box-shadow: 2px 2px 7px #E8E8E8 inset;
  -moz-box-shadow: 2px 2px 7px #E8E8E8 inset;
  box-shadow: 2px 2px 7px #E8E8E8 inset;
}
.contactsubmitbutton {
  height: 25px;
  margin-top: 5px;
}

.popupwrap {
  position: relative;
  background-color: #eef4fb;
  width: 850px;
  height: 450px;
  margin: 20px auto;
  -webkit-border-radius: 9px;
  -webkit-border-top-left-radius: 14px;
  -webkit-border-top-right-radius: 14px;
  -moz-border-radius: 9px;
  -moz-border-radius-topleft: 14px;
  -moz-border-radius-topright: 14px;
  border-radius: 9px;
  border-top-left-radius: 14px;
  border-top-right-radius: 14px;
  -webkit-box-shadow: 0px 0px 15px 0px #000;
  -moz-box-shadow: 0px 0px 15px 0px #000;
  box-shadow: 0px 0px 15px 0px #000;
  background-color: rgb(40, 40, 40);
  font-family: Arimo, Helvetica, sans-serif;
}
.popuptoptitlewrap {
  width: 100%;
  height: 60px;
  -webkit-border-top-left-radius: 9px;
  -webkit-border-top-right-radius: 9px;
  -moz-border-radius-topleft: 9px;
  -moz-border-radius-topright: 9px;
  border-top-left-radius: 9px;
  border-top-right-radius: 9px;
  border-bottom: 1px solid #335c99;
  background-color: #4d89e3;
  text-align: center;
}
.popuptoptitlewrap h1 {
  padding-top: 14px;
  color: rgb(253, 253, 253);
  margin-left: 0px;
  font-size: 26px;
  text-shadow: 0px 0px 3px #4D4D4D;
  letter-spacing: 1px;
}
.json {
  word-wrap: normal;
  word-wrap: break-word;
}
pre {
  background-color: #FCF6DB;
  border: 1px solid #E5E0C6;
  max-height: 300px;
  max-width: 800px;
  width: 764px;
  overflow: auto;
  word-wrap: break-word;
}
.doc-params thead tr th {
  text-align: left;
  border-bottom: 1px solid #999;
}
.doc-params tbody tr td {
  border-bottom: 1px solid #CCC;
}
@-moz-document url-prefix() {
  * {
    font-family: "Arial", sans-serif;
  }
}
.uploadscreen_inputwrap {
  margin: 30px;
  width: 264px;
  height: 360px;
  background-color: rgb(253, 253, 253);
  -webkit-border-radius: 4px;
  -moz-border-radius: 4px;
  border-radius: 4px;
  -webkit-box-shadow: 0 0 8px 0 rgb(200, 200, 200);
  -moz-box-shadow: 0 0 8px 0 rgb(200, 200, 200);
  box-shadow: 0 0 8px 0 rgb(200, 200, 200);
  padding-top: 5px;
  border: 1px solid rgb(200, 200, 200);
  overflow: hidden;
  text-align: center;
}
.uploadscreen_inputbottomwrap {
  width: 100%;
  background-color: rgb(244, 244, 244);
  height: 200px;
  overflow: hidden;
  margin-top: -20px;
  border-top: 1px solid rgb(200, 200, 200);
  padding-top: 38px;
}
.uploadscreen_inputwrap fieldset {
  outline: none;
  border: none;
  margin-top: 6px;
  text-align: center;
}
.screenupload_legend {
  margin: 0 auto;
}
@-moz-document url-prefix() {
  .screenupload_legend {
    margin-left: 60px;
    text-align: left;
  }
}
.thumbnailselectform {
  height: 144px;
  overflow: hidden;
  width: 150px;
  text-align: center;
  -webkit-border-radius: 3px;
  -moz-border-radius: 3px;
  border-radius: 3px;
}
.thumbnailselectform option {
  padding: 2px;
}
.uploadscreenbutton {
  margin-left: 28px;
  margin-top: 2px;
  width: 200px;
}
.uploadscreenpage_notificationmessagewrap {
  margin: 0 auto;
  text-align: center;
  width: 100%;
  display: block;
  background-color: #43d46f;
  padding-top: 5px;
  padding-bottom: 8px;
  box-shadow: 0 3px 9px -7px rgba(0, 0, 0, 0.7);
  margin-top: 20px;
}
.uploadscreenpage_notificationtext {
  color: white;
  font-size: 14px;
}
.uploadscreen_tutorialwrap {
  width: 480px;
  height: 320px;
  background-color: #eef5f6;
  -webkit-box-shadow: 0 0 8px 0 rgb(200, 200, 200);
  -moz-box-shadow: 0 0 8px 0 rgb(200, 200, 200);
  box-shadow: 0 0 8px 0 rgb(200, 200, 200);
}
.uploadscreen_headerwrap {
  width: 100%;
  background-color: rgb(230, 230, 230);
  border-bottom: 1px solid rgb(205, 205, 205);
  height: 35px;
}
.uploadscreen_tutorialtitle {
  font-weight: bold;
  font-size: 21px;
  display: inline-block;
  color: rgb(100, 100, 100);
  vertical-align: top;
  margin-top: 6px;
}
.uploadscreen_magnifyimg {
  height: 24px;
  width: 24px;
  margin: 5px;
  display: inline-block;
}
.uploadscreen_previewimgwrap {
  width: 460px;
  height: 265px;
  margin-top: 7px;
  margin-left: 9px;
  border: 1px solid rgb(170, 170, 170);
}
.pagination-wrap {
  text-align: center;
  margin-top: 15px;
}
.pagination-wrap .page-worded {
  display: inline-block;
  background-color: rgb(100, 100, 100);
  background: #88bfe8;
  background: rgba(62, 201, 217, 1);
  background: -moz-linear-gradient(top, rgba(62, 201, 217, 1) 0%, rgba(65, 176, 217, 1) 100%);
  background: -webkit-gradient(left top, left bottom, color-stop(0%, rgba(62, 201, 217, 1)), color-stop(100%, rgba(65, 176, 217, 1)));
  background: -webkit-linear-gradient(top, rgba(62, 201, 217, 1) 0%, rgba(65, 176, 217, 1) 100%);
  background: -o-linear-gradient(top, rgba(62, 201, 217, 1) 0%, rgba(65, 176, 217, 1) 100%);
  background: -ms-linear-gradient(top, rgba(62, 201, 217, 1) 0%, rgba(65, 176, 217, 1) 100%);
  background: linear-gradient(to bottom, rgba(62, 201, 217, 1) 0%, rgba(65, 176, 217, 1) 100%);
  filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#3ec9d9', endColorstr='#41b0d9', GradientType=0);
  margin: 1px;
  padding: 5px 7px 5px 7px;
  border: 2px solid #1C1C1C;
  color: white;
  -webkit-border-radius: 5px;
  -moz-border-radius: 5px;
  border-radius: 5px;
}
.pagination-wrap .page-worded:hover {
  color: white;
  -o-transition: .1s ease-in-out;
  -ms-transition: .1s ease-in-out;
  -moz-transition: .1s ease-in-out;
  -webkit-transition: .1s ease-in-out;
  transition: .1s ease-in-out;
}
.pagination-wrap .inner-pagination-wrap {
  background-color: rgb(242, 242, 242);
  display: inline-block;
  padding: 5px;
  font-size: 0;
  -webkit-border-radius: 5px;
  -moz-border-radius: 5px;
  border-radius: 5px;
}
.pagination-wrap .page {
  padding: 5px;
  font-size: 16px;
  border-left: 1px solid rgb(242, 242, 242);
  border-right: 1px solid rgb(242, 242, 242);
  font-weight: bold;
  color: black;
}
.pagination-wrap .page:hover,
.pagination-wrap .current {
  background-color: rgb(215, 215, 215);
  border-left: 1px solid rgb(200, 200, 200);
  border-right: 1px solid rgb(200, 200, 200);
  border-bottom: 1px solid rgb(200, 200, 200);
  -o-transition: .1s ease-in-out;
  -ms-transition: .1s ease-in-out;
  -moz-transition: .1s ease-in-out;
  -webkit-transition: .1s ease-in-out;
  transition: .1s ease-in-out;
}

