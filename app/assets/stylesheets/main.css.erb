  background-color:#0f0f0f;
  background-color:#161718;
  width:100%;
  height:40px;
  opacity:0.8;
  position: relative;
  z-index: 5;

}

.logoheaderbar img {
  margin-top:2px;
  display:inline-block;
}

.logowrap ul {
  width:150px;
  float: right;
  height:20px;
}

.logowrap ul, li {
  list-style-type: none;
  list-style-position:inside;
  margin:0;
  padding:0;
}

.logowrap ul li {
  margin-top:16px;
  padding-bottom:6px;
}

.logowrap ul ul{
  display: none;
}

.logowrap ul:hover ul {
  display: block;
}

.logowrap ul ul li {
  color:white;
  background-color:black;
  opacity:0.8;
  padding:5px;
  margin-left:-5px;
  margin-top:0px;
  width:110px;
  font-size:13px;
}

.logowrap ul ul li a {
  color:white;
}

.logowrap .username {
  color:white;
  display: inline-block;
  font-size:15px
}

.logowrap img.downarrow {
  display: inline-block;
  vertical-align: top;
  margin-right:40px;
  margin-top:5px;
  margin-left:6px;
  font-size:15px;
  width:13px;
  cursor: pointer;
}

